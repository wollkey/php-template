{
    "name": "wollkey/php-template",
    "description": "PHP template with comprehensive development tools and configurations",
    "type": "project",
    "license": "MIT",
    "keywords": ["template", "boilerplate", "php", "development"],
    "homepage": "https://github.com/wollkey/php-template",
    "authors": [
        {
            "name": "Alisher Tashmatov",
            "email": "pirojok167@gmail.com",
            "homepage": "https://github.com/wollkey",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/wollkey/php-template/issues",
        "source": "https://github.com/wollkey/php-template"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "8.4.*",
        "symfony/dotenv": "^7.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.88",
        "icanhazstring/composer-unused": "^0.9.5",
        "phpunit/phpunit": "^12.4",
        "rector/rector": "^2.2",
        "vimeo/psalm": "^6.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
